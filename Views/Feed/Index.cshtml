<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="~/css/feedpage.css">
    <script src="~/js/feedpage.js"></script>
    <!-- Font Awesome -->
    <script src="https://kit.fontawesome.com/5b788c8dd4.js" crossorigin="anonymous"></script>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

</head>
<body>
    @{
        Html.RenderPartial("Navbar");
    }
    <div class="container">
        <div class="left-section">
            <!-- Tartalom a bal oldalon -->
            @{
                Html.RenderPartial("LeftPanel");
            }
        </div>
        <div class="separator"></div>
        <div class="middle-section">
            <div class="card post">
                <div class="card-header text-center">
                    <i class="fa-solid fa-at"></i> User
                </div>
                <div class="card-body">
                    <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
                </div>
                <div class="card-footer text-body-secondary text-center">
                    <i class="fa-regular fa-heart postreaction" id="hjhchm" onclick="like('hjhchm');"></i>
                    <i class="fa-solid fa-comment postreaction"></i>
                    <i class="fa-solid fa-share postreaction"></i>
                </div>
            </div>


            <div class="card post">
                <div class="card-header text-center">
                    <i class="fa-solid fa-at"></i> User
                </div>
                <div class="card-body">
                    <img class="post-img" src="https://download.cdn-shop.com/media/web_images/uk/my-picture/structured-data/framed-picture-4x3.jpg" />
                    <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
                </div>
                <div class="card-footer text-body-secondary text-center">
                    <i class="fa-regular fa-heart postreaction" id="dgssdg" onclick="like('dgssdg');"></i>
                    <i class="fa-solid fa-comment postreaction"></i>
                    <i class="fa-solid fa-share postreaction"></i>
                </div>
            </div>
        </div>
        <div class="separator"></div>
        <div class="right-section">
            <!-- Tartalom a jobb oldalon -->
            @{
                Html.RenderPartial("RightPanel");
            }
        </div>
    </div>
</body>
</html>

